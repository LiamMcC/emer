
<%- include('header') -%>
<div class="w3-row w3-padding w3-border">
 
  <% if (isAuthenticated) { %>

    <div class="w3-row-padding w3-padding-16 w3-center" id="food">

<% result.forEach(function(contact) { %>
       

  
    <div class="w3-third">
      <div class="w3-container w3-white">
      <img src="../<%= contact.cImage %>" alt="Sandwich" style="width:100%">
      <h3><%= contact.cName %></h3>
      <p>Went Live On, <span class="w3-opacity"><%= contact.cDate %></span></p>
      <p>Prize <%= contact.cPrize %></p>
      
  <p>You Are Testing This As An Admin. Once you solve all clues the competition will appear live on the site for all users</p>
           


          <div class="w3-justify">

            <p><%= contact.cDescription %></p>
            <p>Testing is on clue: <%= contact.currentMax %></p>
            <p class="w3-right">
              <% if (contact.currentProgress  !== 0 && contact.userName == user.userName ) { %>   
              <a href="intestclue/<%= contact.clueID %>/clue<%= contact.currentProgress %>"><button class="w3-button w3-green" ><b>Play Now x </b> </button></a>
              
              
              <%} else { %>
                <a href="intestclue/<%= contact.clueID %>/clue1"><button class="w3-button w3-green" ><b>Play Now </b> </button></a>
              <% } %>  
      
              <a href="intestclue/<%= contact.clueID %>/clue<%= contact.currentProgress %>"><button class="w3-button w3-black" ><b>Current Guess </b> <span class="w3-tag w3-white"><%= contact.currentMax %></span></button></a></p>
              <p class="w3-clear"></p>
            <div class="w3-row w3-margin-bottom" id="demo1" style="display:none">
              <hr>
              
                <div class="w3-col l10 m9">
                  <h4>Winner <span class="w3-opacity w3-medium"><%= contact.cWinner %></span></h4>
                  
                </div>
            </div>
          </div>
    </div>

    </div>



   
  
  






<% }); %>
</div>

<%} else { %>



  <% result.forEach(function(contact) { %>
       
    

    <!-- Blog entry -->
<div class="w3-container w3-white w3-margin w3-padding-large">
  <div class="w3-center">
    <h3><%= contact.cName %></h3>
    <h5>Went Live On, <span class="w3-opacity"><%= contact.cDate %></span></h5>
    <h3>Prize <%= contact.cPrize %></h3>
    
    <h5> A user is currently on guess no <%= contact.currentMax %> for this competition</h5>
    

      
  </div>

  <div class="w3-justify">
     
      
    <img src="../<%= contact.cImage %>" alt="win logo" style="width:30%" class="w3-padding-16">
    <p><%= contact.cDescription %></p>
    <p class="w3-left"><a href="/like/<%= contact.clueID %>" class="w3-button w3-white w3-border"><b><i class="fa fa-thumbs-up"></i> Like</b></a><button class="w3-button w3-white w3-border" onclick="likeFunction(this)"><b><i class="fa fa-thumbs-up"></i> Like (<%= contact.cLikes %>)</b></button></p>
    <p class="w3-right">
       
      <a href="/login"><button class="w3-button w3-green" ><b>LogIn To Play </b> </button></a>
      
      
     
        
      

      <a href="/login"><button class="w3-button w3-black" ><b>Current Guess </b> <span class="w3-tag w3-white"><%= contact.currentMax %></span></button></a></p>
      <p class="w3-clear"></p>
    <div class="w3-row w3-margin-bottom" id="demo1" style="display:none">
      <hr>
      
        <div class="w3-col l10 m9">
          <h4>Winner <span class="w3-opacity w3-medium"><%= contact.cWinner %></span></h4>
          
        </div>
    </div>
  </div>
</div>
<hr>
 


<% }); %>





  <% } %>


</div>

<%- include('footer') -%>
  