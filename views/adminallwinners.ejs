
<%- include('header') -%>


<div class="w3-row w3-padding w3-border">
<div class="w3-main" style="margin-left:300px"></div>
<% if (user.adminRights == true) { %>
 
  <!-- Team Section -->
<div class="w3-container" style="padding:4px 16px" id="team">
  <h3 class="w3-center">Admin Area Only</h3>
  <p class="w3-center w3-large">Be Careful In Here And Remember To Log Out</p>

  
  <div class="w3-row-padding w3-grayscale" style="margin-top:64px">


    <% result.forEach(function(contact) { %>
    <div class="w3-col l3 m6 w3-margin-bottom">
      <div class="w3-card w3-white" >
        <img src="/w3images/team2.jpg" alt="John" style="width:100%">
        <div class="w3-container">
          <h3><%= contact.userName %></h3>
          
          <p>Won <%= contact.cName %> (<%= contact.comp %>) on <%= contact.dateWon %></p>
          
        </div>
      </div>
    </div>
    <% }); %>


  </div>
</div>


  


    <%} else { %>
      STRANGER DANGER
    <% } %>

    </div>

 
<%- include('footer') -%>
  